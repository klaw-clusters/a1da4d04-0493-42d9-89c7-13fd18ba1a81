{
    "kind": "SeldonDeployment",
    "apiVersion": "machinelearning.seldon.io/v1alpha2",
    "metadata": {
        "name": "mlflow-tom-smith",
        "namespace": "test",
        "creationTimestamp": null,
        "labels": {
            "fluentd": "true"
        }
    },
    "spec": {
        "name": "mlflow-tom-smith",
        "predictors": [
            {
                "name": "default",
                "graph": {
                    "name": "mlflow-tom-smith-container",
                    "implementation": "MLFLOW_SERVER",
                    "endpoint": {
                        "type": "REST"
                    },
                    "modelUri": "gs://tom-seldon-examples/mlflow/model",
                    "logger": {
                        "mode": "all"
                    }
                },
                "componentSpecs": [
                    {
                        "metadata": {
                            "creationTimestamp": "2021-09-28T06:54:29Z"
                        },
                        "spec": {
                            "containers": [
                                {
                                    "name": "mlflow-tom-smith-container",
                                    "resources": {
                                        "limits": {
                                            "cpu": "1",
                                            "memory": "2Gi"
                                        },
                                        "requests": {
                                            "cpu": "1",
                                            "memory": "1Gi"
                                        }
                                    },
                                    "livenessProbe": {
                                        "httpGet": {
                                            "path": "/health/ping",
                                            "port": "http",
                                            "scheme": "HTTP"
                                        },
                                        "initialDelaySeconds": 80,
                                        "periodSeconds": 5,
                                        "successThreshold": 1,
                                        "failureThreshold": 200
                                    },
                                    "readinessProbe": {
                                        "httpGet": {
                                            "path": "/health/ping",
                                            "port": "http",
                                            "scheme": "HTTP"
                                        },
                                        "initialDelaySeconds": 80,
                                        "periodSeconds": 5,
                                        "successThreshold": 1,
                                        "failureThreshold": 200
                                    }
                                }
                            ]
                        }
                    }
                ],
                "replicas": 1,
                "engineResources": {},
                "svcOrchSpec": {},
                "traffic": 100
            }
        ],
        "annotations": {
            "seldon.io/engine-seldon-log-messages-externally": "true"
        },
        "protocol": "seldon",
        "transport": "rest"
    },
    "status": {}
}