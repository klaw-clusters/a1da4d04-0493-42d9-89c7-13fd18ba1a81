{
    "kind": "InferenceService",
    "apiVersion": "serving.kubeflow.org/v1alpha2",
    "metadata": {
        "name": "kfserving-model",
        "namespace": "dev",
        "creationTimestamp": null,
        "labels": {
            "fluentd": "true"
        },
        "annotations": {
            "project.seldon.io/default": "default"
        }
    },
    "spec": {
        "default": {
            "predictor": {
                "sklearn": {
                    "storageUri": "gs://seldon-models/sklearn/income/model",
                    "resources": {
                        "limits": {
                            "cpu": "1",
                            "memory": "1Gi",
                            "nvidia.com/gpu": "0"
                        },
                        "requests": {
                            "cpu": "100m",
                            "memory": "1Gi",
                            "nvidia.com/gpu": "0"
                        }
                    }
                },
                "minReplicas": 0,
                "maxReplicas": 1,
                "logger": {
                    "mode": "all"
                }
            }
        }
    },
    "status": {}
}